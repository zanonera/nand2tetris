/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
	Not(in=a,out=nota);
	And(a=nota, b=b, out=andnota);
	Not(in=b, out=notb);
	And(a=a, b=notb, out=andnotb);
	Or(a=andnota, b=andnotb, out=out);
}